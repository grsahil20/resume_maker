<% if qualifications.present? %>
<div class='qualifications container col-md-12 seperator_blue_top'>
  <div class="col-md-3">
    <h3>Qualifications</h3>
  </div>
  <div class="seperator_blue_left col-md-9">
    <ul class="extra_margin_top">
      <% qualifications.each do |qualification| %>
      <li class="qualification">
        <h4><%= qualification.title %></h4>
        <p>
          <% if qualification.school_name.present?  %>
          <%= qualification.school_name %>
          <% end %>
        </p>
        <% if qualification.description %>
        <p><%= qualification.description %></p>
        <% end %>
        <% if qualification.start_date.present? || qualification.end_date.present? %>
        <p>
          <%= rangify_dates(qualification.start_date,  qualification.end_date) %>
        </p>
        <% end %>
      </li>
      <% end %>
    </ul>
  </div>
  <% end %>
</div>
